- result = @box.import_result.split(';')
- total = result[0]
- added = result[1]
- new = result[2]
- failed = result[3]
- missing_brands = result[4]
- if @polishes
  $('#secondaryResults').html('#{j(render @polishes, big_polish: true).html_safe}');
- if failed.to_i > 0
  $('#secondaryResults').prepend('#{content_tag :p, t('user.import_msg.added', count: added) + t('user.import_msg.failed', count: failed) + t('user.import_msg.unknown', count: missing_brands.size, brands: missing_brands)}');
- else
  $('#secondaryResults').prepend('#{content_tag :p, t('user.import_msg.ok', count: added)}');
